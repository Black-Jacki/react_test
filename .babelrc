{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-proposal-class-properties", // 启用箭头函数
      ["import",{ 
        "libraryName": "antd",  
        "libraryDirectory":"es",
        "style": "css" // style: true 会加载 less 文件
      }]
    ]
  }